{
  "name": "lowcode-material-ant-vue",
  "version": "0.0.9",
  "description": "Ant design vue for lowcode",
  "main": "./dist/index.min.js",
  "module": "./dist/lowcode-material-ant-vue.mjs",
  "typings": "./dist/lowcode-material-ant-vue.d.ts",
  "scripts": {
    "start": "vue-cli-service lowcode:dev --config ./build.lowcode.js",
    "build": "vue-cli-service lowcode:build"
  },
  "files": [
    "dist/",
    "build/",
    "src/lowcode/"
  ],
  "dependencies": {
    "@alilc/lowcode-types": "^1.1.6",
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.19",
    "core-js": "^3.30.1",
    "dayjs": "^1.11.7",
    "less-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@alifd/build-plugin-lowcode": "^0.3.16",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@knxcloud/vue-cli-plugin-lowcode": "^1.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "lint-staged": "^13.2.2",
    "moment": "^2.29.4",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "~4.8.3",
    "vue-tsc": "^0.40.13",
    "webpack": "^5.81.0"
  },
  "gitHooks": {
    "pre-commit": "set NODE_ENV=production npx lint-staged && npx vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{less,scss,css,vue}": [
      "stylelint --fix"
    ]
  }
}
